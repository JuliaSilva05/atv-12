//leia uma matriz 4x4, conte e escreva quantos 
//valores maiores que 10 ela possui

#include <stdio.h>

int main(){
    int contador = 0;
    int matriz[4][4] = {
        {6,20,32,4},
        {55,11,24,9},
        {3,30,26,9},
        {12,2,14,7}};
    for (int row = 0;row<4;row++){
        for (int column = 0;column<4;column++){
            if (matriz[row][column] > 10){
                contador++;
                printf("%d, ",matriz[row][column]);
            }
        }
        printf("\n");
    }
    printf("\n%d numeros sao maiores que 10\n",contador);
}
