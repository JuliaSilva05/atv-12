//declare uma matriz 5x5. preencha com 1 a diagonal principal 
//e com 0 os demais elementos. escreva ao final a matriz obtida

#include <stdio.h>

int main(){
    
    int matriz[5][5] = {
        {6,20,32,4,7},
        {55,11,24,9,45},
        {3,30,26,9,80},
        {12,2,14,7,34}};
    for (int row = 0;row<5;row++){
        for (int column = 0;column<5;column++){
            if (row == column){
                matriz[row][column] = 1;
                printf("%d, ",matriz[row][column]);
            } else{
                matriz[row][column] = 0;
                printf("%d, ",matriz[row][column]);
            }
        }
        printf("\n");
    }
}
